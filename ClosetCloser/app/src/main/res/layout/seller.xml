<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <Button
        android:id="@+id/btn_regselleritem"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:layout_above="@+id/image111"
        android:layout_alignEnd="@+id/image111"
        android:layout_alignRight="@+id/image111"
        android:layout_marginLeft="41dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="-212dp"
        android:background="@color/colorPrimary"
        android:text="상품등록 "
        android:textColor="@color/colorWhite"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/editselleritemurl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/image111"
        android:layout_alignBottom="@+id/selleritemname"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="-118dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/editselleritemprice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/image111"
        android:layout_alignBottom="@+id/selleritemname"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="-59dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/selleritemname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView8"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginBottom="38dp"
        android:text="상품이름:" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView7"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginBottom="40dp"
        android:text="상품가격: " />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView6"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="42dp"
        android:layout_marginLeft="42dp"
        android:layout_marginBottom="39dp"
        android:text="상품 Url: " />

    <Button
        android:id="@+id/button222"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_above="@+id/image111"
        android:layout_alignEnd="@+id/image111"
        android:layout_alignRight="@+id/image111"
        android:layout_marginLeft="41dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="-77dp"
        android:layout_marginRight="-77dp"
        android:layout_marginBottom="21dp"
        android:background="@color/colorPrimary"
        android:text="Camera Open"
        android:textColor="@color/colorWhite"
        android:textSize="18dp" />

    <Button
        android:id="@+id/button111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_above="@+id/image111"
        android:layout_marginLeft="41dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="20dp"
        android:background="@color/colorPrimary"
        android:text="Gallery Open"
        android:textColor="@color/colorWhite"
        android:textSize="18dp" />


    <ImageView
        android:id="@+id/image111"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="113dp"
        android:layout_marginRight="113dp"
        android:layout_marginBottom="108dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginBottom="338dp"
        android:gravity="center"
        android:text="상품사진: " />

    <EditText
        android:id="@+id/editselleritemname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/image111"
        android:layout_alignBottom="@+id/selleritemname"
        android:ems="10"
        android:inputType="textPersonName" />


</RelativeLayout>